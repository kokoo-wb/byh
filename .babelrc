{
  "presets": [
    "es2015",
    "react",
    "stage-2"
  ],
  "plugins": [
    [
      "antd",
      [
        { "libraryName": "antd" },
        {
          "style": "css",
          "libraryName": "antd-mobile"
        }
      ]
    ],
    ["react-intl", {
        "messagesDir": "./build/messages/"
    }]
  ],
  "env": {
    "start": {
      "presets": [
        "react-hmre"
      ]
    }
  }
}